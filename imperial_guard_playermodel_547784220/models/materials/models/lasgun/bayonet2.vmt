{
$baseTexture "models\lasgun\bayonet"
$bumpmap "models\lasgun\bayonet_normal2"
$nocull 1
	"$phong" "1"
	"$phongexponent" "150"
	"$phongboost" "1"	
 	"$phongfresnelranges" "[0.1 2.1 5.1]"
